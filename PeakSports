			//global init for jquery
			head.ready("jquery", function() {
				head.js(
					//jquery-dependent libraries					
					{bootstrap: "/static/js/bootstrap.min.js?v=20160201"},					{housejs: "/static/js/thehouse.js?v=20161018"},
					//jquery plugins
					{jquery_shadowbox: "/static/js/shadowbox.js"},
					{easing: "/static/js/easing.js"},
					{hover_intent: "/static/js/jquery.hoverIntent.min.js"},
					//menu plugins
					{modernizr: "/static/js/modernizr.custom.js"},
					{accordion: "/static/js/accordionMenu.js?v=20160629"},
					{lazyload: "/static/js/jquery.lazyload.js"}
				);
			});
			if (screen.width > 768) {
				head.ready("jquery", function() {
					$("body").append('<a href="#" id="toTop" class="hidden-xs hidden-print" style="display:inline; z-index:2000;"><span id="toTopHover" style="opacity:0;"></span>To Top</a>');
					head.js(
						{totop: "/static/js/jquery.ui.totop.min.js"}
					);
					head.ready("totop", function() {
						$(document).UItoTop({ easingType: 'easeOutQuart' });
					});
				});
			}
			//global init for jquery_shadowbox
			head.ready("jquery_shadowbox", function() {
				Shadowbox.init({
					troubleElements: ["select", "object", "embed", "canvas", "iframe"]
				});

				//Check for and Shadowbox any old colorbox links
				if($("a.cpModal").length) {
					Shadowbox.setup($("a.cpModal"));
				}

				$('#largeimglink').click( function() {
				 var currentImage = $('.cloudzoom-gallery-active').attr('href');
				 var cacheItem = null;
				 var cache = Shadowbox.cache;

				
				  var mainImage = $('#prdimg').attr('src').replace("-prod.jpg","-zoom.jpg");			
				 
				  if(!$('.cloudzoom-gallery-active').attr('href')) {
				    currentImage = $('#product-images a.cloudzoom-gallery:first').attr('href');
				  }
				

				  for (var p in cache) {
					if (cache.hasOwnProperty(p)) {
						cacheItem = cache[p];
						if (cacheItem.content == currentImage) {
							Shadowbox.open(cacheItem);
						} else {
							if (currentImage === undefined || currentImage === null) {
								Shadowbox.open({
									content: mainImage,
									type: "image",
									player: "img",
									height: 600,
									width: 600
								}); 
							}
						}
					}
				  }
				  return false;
				});
			});

			// init lazyload
			head.ready(document, function() {
				head.ready("lazyload", function() {
					$("img.lazy").lazyload();
					$("#mp-pusher").scroll();
				});
			});
			
			//global init for DOM ready
			head.ready(document, function() {
				head.ready("bootstrap", function() {
					$(function() {
						//This set of code checks for a search page with refinables and collapses the refines on smaller devices
						if ($("#search-refine").length > 0) {
							var resizeTimer; //Set resizeTimer to empty so it resets on page load
							var windowWidth;  //Set windowWidth so we can track changes to the width

							function searchCollapse() {
								if (windowWidth != $(window).width()) {
									if ($(window).width() < 768) {
										//Collapse the search refines and search refine list on xsmall devices
										$("#search-refine").collapse("hide");
								//		$("#search-refine .panel-collapse").collapse("hide");
									}else if ($(window).width() < 977) {
										//Be sure to show search refine bar if coming out of xs device 
										if (!$("#search-refine").hasClass("in")) {
											$("#search-refine").collapse("show");
										}
									}else{
										//Collapse everything except those chosen and always Vendor or Category or Gender if exists
										if (!$("#search-refine").hasClass("in")) {
											$("#search-refine").collapse("show");
										}
										if ($("#collapse-Vendor").length > 0) {
											$("#collapse-Vendor").collapse("show");
										}
										if ($("#collapse-Category").length > 0) {
											$("#collapse-Category").collapse("show");
										}
										if ($("#collapse-Gender").length > 0) {
											$("#collapse-Gender").collapse("show");
										}
									}
									
									windowWidth = $(window).width();
								}
							}

							// On resize, run the function and reset the timeout
						    // add a 250 ms delay between calls to improve performance
						    $(window).resize(function() {
						        clearTimeout(resizeTimer);
						        resizeTimer = setTimeout(searchCollapse, 250);
						    });

						    searchCollapse();
						}
					});
				});
			});

		<script type="text/javascript">
			function SetupHoverIntent(MainMenu, SubMenu){				
				$("#" + MainMenu).hoverIntent({   
					sensitivity: 1, // number = sensitivity threshold (must be 1 or higher)    
					interval: 25,  // number = milliseconds for onMouseOver polling interval    
					timeout: 100,   // number = milliseconds delay before onMouseOut    
					over:function(){
						if ($(window).width() > 995) {//Make sure hoverIntent code don't get tricker on Small devices						
							
							var SubMenuList = new Array("SnowboardSubMenu", "SkiSubMenu","WakeSubMenu","MenSubMenu","WomenSubMenu", "KidSubMenu", "OutletSubMenu","BikeSubMenu","FootwearSubMenu","BrandSubMenu", "BagSubMenu","SkateSubMenu","CampingSubMen","WatersportSubMenu","MoreSubMenu", "SunglassSubMenu");
							var MainMenuList = new Array("SnowboardMenu", "SkiMenu","WakeMenu","MensMenu","WomensMenu","KidsMenu","OutletMenu","BikeMenu","FootwearMenu","BrandsMenu","BagsMenu","SkateMenu","CampingMenu","WatersportMenu","MoreMenu", "SunglassMenu");
							
							for (i=0; i < SubMenuList.length; i++) {
								if (SubMenuList[i] != SubMenu) {
									$("#" + MainMenuList[i]).removeClass('dropdown-toggle');
									$("#" + SubMenuList[i]).hide();
								}
								else{
									$("#" + SubMenu).addClass('dropdown-menu');
									$("#" + SubMenu).show();									
								}
							}
						}
					},
					out: function(){						
						if (!$('.header').is(':hover')) {//here we make sure that no trigger submenu when move away top from navbar
							if ($(window).width() > 995){
								$("#" + MainMenu).removeClass('dropdown-toggle');
								$("#" + SubMenu).hide();
							};				
						}
						else{
							$("#" + SubMenu).hoverIntent({   
							sensitivity: 1, // number = sensitivity threshold (must be 1 or higher)    
							interval: 1,  // number = milliseconds for onMouseOver polling interval    
							timeout: 1,   // number = milliseconds delay before onMouseOut    					
								over: function(){},
								out: function(){
									if ($(window).width() > 995) {//Make sure hoverIntent code don't get tricker on Small devices	
									$("#" + SubMenu).hide();
									$("#" + MainMenu).removeClass('dropdown-toggle');
									$("#" + SubMenu).removeClass('dropdown-menu');
									}
								}					
							});
						}
					}
				});				
			};
						
			head.ready(function() {
				SetupHoverIntent("SnowboardMenu", "SnowboardSubMenu");
				SetupHoverIntent("SunglassMenu", "SunglassSubMenu");
				SetupHoverIntent("SkiMenu", "SkiSubMenu");
				SetupHoverIntent("WakeMenu", "WakeSubMenu");
				SetupHoverIntent("MensMenu", "MenSubMenu");
				SetupHoverIntent("WomensMenu", "WomenSubMenu");
				SetupHoverIntent("KidsMenu", "KidSubMenu");
				SetupHoverIntent("OutletMenu", "OutletSubMenu");
				SetupHoverIntent("BikeMenu", "BikeSubMenu");
				SetupHoverIntent("FootwearMenu", "FootwearSubMenu");
				SetupHoverIntent("BagsMenu", "BagSubMenu");
				SetupHoverIntent("SkateMenu", "SkateSubMenu");
				SetupHoverIntent("CampingMenu", "CampingSubMen");
				SetupHoverIntent("WatersportMenu", "WatersportSubMenu");
				SetupHoverIntent("BrandsMenu", "BrandSubMenu");
				SetupHoverIntent("MoreMenu", "MoreSubMenu");
			});
		   </script><script type="text/javascript">
		var total = 0;
		var items = '';
		var cart = '';
		if (document.cookie.search('cart_contents=') > -1) {
			cart = JSON.parse("["+document.cookie.split('cart_contents=').pop().split(";").shift()+"]");
			cart.forEach(function(prod) {
				for(var key in prod) {
					if (prod.hasOwnProperty(key)) {
					items += "{name:'" + key.replace("\'", "") + "', value:'" + prod[key].prod.replace("\'", "") + "'},";
					total += prod[key].sub;
					}
				}
			});
		}
		head.ready(function() {
		if (screen.width > 768) {
			window.__lc = window.__lc || {};
			window.__lc.license = 5248121;
			window.__lc.ga_version = "ga";
			window.__lc.params = [
				{name: 'Vendor', value: ''},
				{name: 'ShopCategory', value: ''},
				{name: 'Shop', value: ''},
				{name: 'Type_id', value: '12'},
				{name: 'URL', value: 'http://www.the-house.com/sitemap.html'},
				{name: 'CartStep', value: ''},
				{name: 'CartTotal', value: total.toFixed(2)},
				{name: 'CartItems', value: items},
			];

			(function() {
				var lc = document.createElement('script'); lc.type = 'text/javascript'; lc.async = true;
				lc.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'cdn.livechatinc.com/tracking.js';
				var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(lc, s);
			})();
			window.LC_API = window.LC_API || {};
			window.LC_API.on_after_load = function() {
				if(document.getElementById('livechat-compact-container') != undefined ) {
					document.getElementById('livechat-compact-container').setAttribute('class', 'hidden-xs');
					document.getElementById('livechat-compact-container').setAttribute('class', 'hidden-print');
					document.getElementById('livechat-compact-container').style.height = '60px';
					document.getElementById('livechat-compact-container').style.width = '225px';
				};
				if(document.getElementById('livechat-full') != undefined ) {
					document.getElementById('livechat-full').setAttribute('class', 'hidden-xs');
				};
	
				$('#livechat-compact-view').contents().find('#content').css("background-color", "#016D9E");
				$('#livechat-compact-view').contents().find('#full-view-button').html("<span style='padding:6px;'><img src='/static/img/live-chat-icon.jpg' alt='The House' /></span><span style='padding:6px;'> Live Chat</span>");
			};
		}
		});
</script>	</body>
</html>

